{
	"extends": "tsconfig/tsconfig.json",
	"compilerOptions": {
		"lib": ["DOM"],
		"module": "ESNext",
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "useDefineForClassFields": true,
  },
  "include": ["src"]
}
